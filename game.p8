--0 - find and talk
--1 - initial attack
--2 - run and defend
--3 - strong run and defend
--4 - safe flower
--5 - running hell
--6 - true ending
segment = 0

end

function _update()

--manage talk
if btn(3) then
 player.talk =
  max(player.talk+3,800)
else
 player.talk =
  min(player.talk-3,0)
end

end

