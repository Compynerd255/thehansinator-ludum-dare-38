function _init()

player = {}
monsters = {}
prog = 0
timer = 0
segment = 0

end

function _update()

--manage talk
if btn(3) then
 player.talk =
  max(player.talk+3,800)
else
 player.talk =
  min(player.talk-3,0)
end

end

function _draw()

--draw character

--draw background and
--surroundings based on talk

end

function player_create()

local player = {}
player.x = 20
player.belief = 1000
player.talk = 0
player.stress = 0
player.hp = 1000
return player

end

function monster_create(d)

local monster = {}
if (d==0) then 
 monster.x = prog-12
elseif (d==1) then
 monster.x = prog+132
end

end
